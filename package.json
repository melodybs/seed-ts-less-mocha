{
  "name": "vue-seed",
  "version": "0.1.0",
  "private": true,
  "author": "jm.kim",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-storybook": "build-storybook",
    "storybook": "start-storybook -p 6006",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit --require tests/unit/setup.ts"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "es6-promise": "^4.2.6",
    "register-service-worker": "^1.6.2",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-i18n": "^8.11.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vuedraggable": "^2.21.0",
    "vuetify": "^1.5.5",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@storybook/addon-a11y": "^5.1.3",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-backgrounds": "^5.1.3",
    "@storybook/addon-console": "^1.1.0",
    "@storybook/addon-knobs": "^5.1.3",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addon-notes": "^5.1.3",
    "@storybook/addon-storyshots": "^5.1.3",
    "@storybook/addon-storysource": "^5.1.3",
    "@storybook/addon-viewport": "^5.1.3",
    "@storybook/addons": "^5.0.11",
    "@storybook/vue": "^5.0.11",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.0",
    "@types/express": "^4.17.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.7",
    "@types/sinon": "^7.0.12",
    "@types/sinon-chai": "^3.2.2",
    "@types/storybook__addon-a11y": "^5.0.0",
    "@types/storybook__addon-backgrounds": "^4.1.2",
    "@types/storybook__addon-knobs": "^5.0.1",
    "@types/storybook__addon-storyshots": "^4.0.1",
    "@types/storybook__addon-viewport": "^4.1.1",
    "@types/storybook__vue": "^5.0.2",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-e2e-cypress": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-plugin-pwa": "^3.8.0",
    "@vue/cli-plugin-typescript": "^3.8.0",
    "@vue/cli-plugin-unit-mocha": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-standard": "^4.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "babel-preset-vue": "^2.0.2",
    "body-parser": "^1.19.0",
    "chai": "^4.1.2",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "expect": "^24.8.0",
    "flush-promises": "^1.0.2",
    "inject-loader": "^4.0.1",
    "jsdom": "^15.1.1",
    "jsdom-global": "^3.0.2",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.5",
    "mocha": "^6.1.4",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "typescript": "^3.4.3",
    "vue-cli-plugin-storybook": "^0.6.1",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.0.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/standard",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "vue/array-bracket-spacing": "warning",
      "vue/arrow-spacing": "warning",
      "vue/block-spacing": "warning",
      "vue/brace-style": "warning",
      "vue/camelcase": "warning",
      "vue/comma-dangle": "warning",
      "vue/component-name-in-template-casing": "warning",
      "vue/eqeqeq": "warning",
      "vue/key-spacing": "warning",
      "vue/match-component-file-name": "warning",
      "vue/no-boolean-default": "warning",
      "vue/no-restricted-syntax": "warning",
      "vue/object-curly-spacing": "warning",
      "vue/require-direct-export": "warning",
      "vue/script-indent": "warning",
      "vue/space-infix-ops": "warning",
      "vue/space-unary-ops": "warning",
      "vue/v-on-function-call": "warning"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bugs": {
    "url": "https://github.com/melodybs/seed-ts-less-mocha/issues"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "homepage": "https://github.com/melodybs/seed-ts-less-mocha#readme",
  "license": "MIT",
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/melodybs/seed-ts-less-mocha.git"
  }
}
